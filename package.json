{
  "name": "hello-books",
  "version": "1.0.0",
  "description": "Hello-Books is a simple application that helps manage a library and its processes like stocking, tracking and renting books. With this application users are able to find and rent books.",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=production&& ./node_modules/.bin/babel-node ./server/server.js",
    "seed": "SET NODE_ENV=production&& babel-node ./server/seeders/add-user-admin",
    "seed:dev": "babel-node ./server/seeders/add-user-admin",
    "start:dev": "nodemon ./server/server.js --exec babel-node --presets env",
    "test": "NODE_ENV=test&& mocha tests/test/* --compilers babel-core/register --timeout 10000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solnsumei/hello-books.git"
  },
  "keywords": [
    "hellobooks",
    "library"
  ],
  "author": "Solomon Nsumei",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/solnsumei/hello-books/issues"
  },
  "homepage": "https://github.com/solnsumei/hello-books#readme",
  "devDependencies": {
    "assert": "^1.4.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "chai": "^4.1.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-validator": "^3.2.1",
    "jsonwebtoken": "^7.4.1",
    "mocha": "^3.5.0",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "pg": "^7.0.2",
    "pg-hstore": "^2.3.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.2.0",
    "sequelize": "^4.4.2",
    "slug": "^0.9.1"
  }
